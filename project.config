[access]
	inheritFrom = v8
[access "refs/branch-heads/*"]
	create = group v8-admins
	forgeAuthor = group v8-admins
	forgeCommitter = group v8-admins
	push = group v8-committers
[access "refs/heads/*"]
	create = group v8-admins
	forgeAuthor = group v8-admins
	forgeCommitter = group v8-admins
	push = group v8-committers
[access "refs/tags/*"]
	create = group v8-admins
	push = group v8-admins
