#include 'src/objects/js-duration-format.h'

type JSDurationFormatField extends int32
constexpr 'JSDurationFormat::Field';
bitfield struct JSDurationFormatFlags extends uint31 {
  largest_unit: JSDurationFormatField: 4 bit;
  smallest_unit: JSDurationFormatField: 4 bit;
}

@generateCppClass
extern class JSDurationFormat extends JSObject {
  locale: String;
  icu_number_formatter:
      Foreign;  // Managed<icu::number::LocalizedNumberFormatter>
  flags: SmiTagged<JSDurationFormatFlags>;
}
